{
    "name": "code-time",
    "displayName": "Code Time",
    "main": "./lib/Activator",
    "version": "0.2.4",
    "scripts": {
        "test": "mocha"
    },
    "description": "Music Time is an open source plugin that curates and launches playlists for coding right from your editor.",
    "keywords": [
        "code time",
        "time",
        "software",
        "software.com",
        "code insights",
        "productivity",
        "coding activity",
        "virtual manager",
        "keystrokes",
        "calendar",
        "analytics"
    ],
    "repository": "https://github.com/swdotcom/swdc-atom",
    "license": "Reproduction of this material is strictly forbidden unless prior written permission is obtained from Software.co Technologies, Inc.",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "configSchema": {
        "showMusicMetrics": {
            "title": "Show Code Time music metrics",
            "description": "Code Time weekly music metrics and the top 40",
            "type": "boolean",
            "default": false,
            "order": 1
        },
        "showGitMetrics": {
            "title": "Show Code Time git metrics",
            "description": "Code Time weekly git commit metrics",
            "type": "boolean",
            "default": false,
            "order": 2
        },
        "showWeeklyRanking": {
            "title": "Show Code Time weekly rankings",
            "description": "Code Time weekly user rankings",
            "type": "boolean",
            "default": false,
            "order": 3
        }
    },
    "dependencies": {
        "axios": "^0.19.0",
        "cody-music": "^2.6.66",
        "crypto": "^1.0.1",
        "font-awesome": "^4.7.0",
        "itunes-node-applescript": "0.3.5",
        "jquery": "3.4.1",
        "lodash": "^4.17.15",
        "moment-timezone": "^0.5.25",
        "open": "6.3.0",
        "spotify-node-applescript": "1.1.1",
        "status-bar": "^2.0.3",
        "vue": "^2.6.10"
    },
    "devDependencies": {
        "mocha": "^6.2.2",
        "typescript": "^2.7.2"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Code Time",
            "properties": {
                "showMusicMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Music metrics"
                },
                "showGitMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Git metrics"
                },
                "showWeeklyRanking": {
                    "type": "boolean",
                    "default": false,
                    "description": "Weekly Ranking"
                }
            }
        },
        "commands": []
    }
}